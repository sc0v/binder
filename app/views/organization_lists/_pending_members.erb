<table class="pending-members-table table table-striped">
  <thead>
    <tr>
      <th>andrew ID</th>
    </tr>
  </thead>
  <% @pending_memberships.each do |member| %>
    <tr id="<%= member.id %>">
      <td><%= member.andrew_id %></td>
      <td><%= link_to "Delete", organization_list_path(member), remote: true, method: :delete, id: member.id.to_s, class: "pending-member btn btn-danger" %></td>
</td>
    </tr>
  <% end %>
</table>