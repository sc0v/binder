<% page ||= "default" %>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <%# items.each do |item| %>
    <%# cache [page, item] do %>
    <%= render partial: 'item', collection: items %>
    <%# end %>
    <%# end %>
  </tbody>
</table>
